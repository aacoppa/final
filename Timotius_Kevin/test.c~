#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <unistd.h>
#include <fcntl.h>
#include <sys/types.h>
#include <sys/socket.h>
#include <netinet/in.h>

int main(){
  int fdw, bytesRead;
  char** temp;
  char** white;
  char* s;
  char* buffer = "0";
  int c;
  char* cardsw;
  fdw = open("white.txt",O_RDONLY,0664);
  bytesRead = read(fdw,cardsw,sizeof(cardsw));
  temp = white;
  char *line1 = cardsw;
  while(s = strsep(&line1,",")){
    *temp = s;
    *temp++;
  }
  close(fdw);
  if(strncmp(buffer,"0",sizeof(buffer)) == 0)
    for(c = 0;c < 5;c++)
      strcat(s,white[rand()%537]);
}
